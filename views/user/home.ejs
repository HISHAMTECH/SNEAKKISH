.dropdown {
    display: inline-block; /* Allows the dropdown to respect text-align: center */
    position: relative; /* Needed for dropdown-content positioning */
}
.dropdown-content {
    display: none;
    position: absolute;
    background-color: white;
    min-width: 160px;
    box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
    z-index: 1000;
    right: 0;
    border-radius: 4px;
    margin-top: 5px;
}

.dropdown-content {
    display: none;
    position: absolute;
    background-color: white;
    min-width: 160px;
    box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
    z-index: 1000;
    right: 0;
    border-radius: 4px;
    margin-top: 5px;
    text-align: center; /* Add this to center all content in the dropdown */
}

.dropdown-content a {
    color: black;
    padding: 10px 16px;
    text-decoration: none;
    display: block;
    transition: background-color 0.3s;
    cursor: pointer;
    /* Remove text-align: center here, as it’s now handled by the parent */
}

.dropdown-content a:hover {
    background-color: #f1f1f1;
}

/* Show dropdown when button is clicked */
.dropdown.active .dropdown-content {
    display: block;
}

.dropdown-btn {
    cursor: pointer;  
}

.product__carousel .product__item {
    text-align: center;
}

.product__carousel .owl-nav button {
